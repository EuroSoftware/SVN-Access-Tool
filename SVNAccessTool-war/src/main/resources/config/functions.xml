    <function id="combobox" function-type-id="combobox-function-type" function-group-id="default" />
    <function id="button" function-type-id="button-function-type" function-group-id="default" />
    <function id="datetime" function-type-id="datetime-function-type" function-group-id="default" />
    <function id="progress-indicator" function-type-id="progress-indicator-function-type" function-group-id="default" />
    <function id="panel" function-type-id="panel-function-type" function-group-id="default" />
    <function id="message-area" function-type-id="message-area-function-type" function-group-id="default" />
    <function id="table" function-type-id="table-function-type" function-group-id="default" />
    <function id="editable-table" function-type-id="editable-table-function-type" function-group-id="default" />
    <function id="text-field" function-type-id="text-field-function-type" function-group-id="default" />
    <function id="composite-actions" function-type-id="composite-actions-function-type" function-group-id="default" />
    <function id="hitbox" function-type-id="hit-box-function-type" function-group-id="default" />
