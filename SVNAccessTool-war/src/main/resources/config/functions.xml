    <function id="dashboard" function-type-id="dashboard-function-type" function-group-id="default" />
    <function id="paging-table" function-type-id="paging-table-function-type" function-group-id="default" />
    <function id="filter-table" function-type-id="filter-table-function-type" function-group-id="default" />
    <function id="toolbox" function-type-id="toolbox-function-type" function-group-id="default" />
    <function id="protected-restricted-access" function-type-id="protected-function-type" function-group-id="default" />
    <function id="protected-full-access" function-type-id="protected-function-type" function-group-id="admin">
      <parameters>
        <parameter name="FULL_PERMISSION" type="java.lang.Boolean" value="true" />
      </parameters>
    </function>
    <function id="overview-write-off" function-type-id="overview-write-off-function-type" function-group-id="application">
      <parameters>
        <parameter name="TABLE_HEIGHT" type="java.lang.String" value="150px" />
      </parameters>
    </function>
    <function id="report-preview" function-type-id="report-preview-function-type" function-group-id="default" />
    <function id="context-popup-view" function-type-id="context-popup-view-function-type" function-group-id="default" />
