<?xml version="1.0" encoding="utf-8"?>
<functions
  xmlns="http://www.gk-software.com/schema/web-vaadin/it-functions/1.0"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="
    http://www.gk-software.com/schema/web-vaadin/it-functions/1.0
    http://www.gk-software.com/schema/web-vaadin/it-functions/function-1.0.xsd
  "
>

  <function-groups>
    <function-group id="default">
      <roles>
        <role>ROLE_USER</role>
      </roles>
    </function-group>
    <function-group id="admin">
      <roles>
        <role>ROLE_ADMIN</role>
      </roles>
    </function-group>
    <function-group id="application">
      <roles>
        <role>ROLE_USER</role>
      </roles>
    </function-group>
  </function-groups>

  <function-list>
    <function id="focus" function-type-id="focus-function-type" function-group-id="default" />
    <function id="binding" function-type-id="binding-function-type" function-group-id="default" />
    <function id="dialogs" function-type-id="dialogs-function-type" function-group-id="default" />
    <function id="popup-panel" function-type-id="popup-panel-function-type" function-group-id="default" />
    <function id="tabsheet" function-type-id="tabsheet-function-type" function-group-id="default" />
